msgstr "GTerminal"

#: ../src/gterminal.vala:564
msgid "Missing command"
msgstr "遺失指令"

#: ../src/gterminal.vala:573
#, c-format
msgid "Unknown command \"%s\""
msgstr "未知指令「%s」"

#: ../src/gterminal.vala:577
#, c-format
